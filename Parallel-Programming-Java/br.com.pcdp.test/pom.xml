<project xmlns="http://maven.apache.org/POM/4.0.0"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<groupId>br.com.pcdp.test</groupId>
	<artifactId>br.com.pcdp.test</artifactId>
	<version>0.0.1-SNAPSHOT</version>
	<name>Ex01-pdcp-test</name>
	<properties>
		<pcdp.version>0.0.4-SNAPSHOT</pcdp.version>
	</properties>

	<repositories>
		<repository>
			<id>pcdp-repo</id>
			<url>https://raw.github.com/habanero-maven/hjlib-maven-repo/mvn-repo-pcdp-${pcdp.version}/</url>
			<snapshots>
				<enabled>true</enabled>
				<updatePolicy>always</updatePolicy>
			</snapshots>
		</repository>
	</repositories>

	<dependencies>
		<dependency>
			<groupId>edu.rice.pcdp</groupId>
			<artifactId>pcdp-core</artifactId>
			<version>${pcdp.version}</version>
		</dependency>
	</dependencies>
</project>